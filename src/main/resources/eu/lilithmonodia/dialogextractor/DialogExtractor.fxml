<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns:fx="http://javafx.com/fxml"
            xmlns="http://javafx.com/javafx"
            fx:controller="eu.lilithmonodia.dialogextractor.DialogExtractorController"
            prefHeight="400.0" prefWidth="600.0">
    <VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:id="root" alignment="CENTER"
          spacing="10.0">
        <Button fx:id="extractButton" text="Extract"/>
        <TextArea fx:id="originalContentArea" editable="false" promptText="Original content will be displayed here..."
                  wrapText="true"/>
        <TextArea fx:id="processedContentArea" editable="false" promptText="Processed content will appear here..."
                  wrapText="true"/>
    </VBox>
</AnchorPane>
